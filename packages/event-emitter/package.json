{
  "name": "event-emitter",
  "version": "0.1.0",
  "private": true,
  "description": "Generic typed event emitter for Real-Router",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/esm/index.d.mts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": {
        "import": "./dist/esm/index.d.mts",
        "require": "./dist/cjs/index.d.ts"
      },
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greydragon888/real-router.git"
  },
  "author": {
    "name": "Oleg Ivanov",
    "email": "greydragon888@gmail.com",
    "url": "https://github.com/greydragon888"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "bench": "NODE_OPTIONS='--expose-gc --max-old-space-size=4096' npx tsx tests/benchmarks/index.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint --cache --ext .ts src/ tests/ --fix --max-warnings 0",
    "test": "vitest run"
  },
  "sideEffects": false
}
