{
  "name": "@real-router/logger",
  "version": "0.2.0",
  "type": "commonjs",
  "description": "Isomorphic logger for Real-Router with level filtering and custom callbacks",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/esm/index.d.mts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": {
        "import": "./dist/esm/index.d.mts",
        "require": "./dist/cjs/index.d.ts"
      },
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "homepage": "https://github.com/greydragon888/real-router",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greydragon888/real-router.git"
  },
  "bugs": {
    "url": "https://github.com/greydragon888/real-router/issues"
  },
  "keywords": [
    "logger",
    "logging",
    "isomorphic"
  ],
  "author": {
    "name": "Oleg Ivanov",
    "email": "greydragon888@gmail.com",
    "url": "https://github.com/greydragon888"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "lint": "eslint --cache --ext .ts src/ tests/ --fix --max-warnings 0",
    "test": "vitest run",
    "test:properties": "vitest run --config vitest.config.properties.mts",
    "bench": "NODE_OPTIONS='--expose-gc --max-old-space-size=4096' npx tsx tests/benchmarks/index.ts",
    "test:mutation": "stryker run",
    "test:mutation:report": "open reports/mutation-report.html"
  },
  "sideEffects": false
}
