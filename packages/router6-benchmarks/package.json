{
  "name": "router6-benchmarks",
  "version": "1.0.0",
  "private": true,
  "description": "Benchmark tests for Router6",
  "scripts": {
    "prebench": "pnpm --filter router6 build",
    "bench": "BENCH_ROUTER=router6 NODE_OPTIONS='--expose-gc --max-old-space-size=8192' npx tsx modules/index.ts",
    "bench:router5": "BENCH_ROUTER=router5 NODE_OPTIONS='--expose-gc --max-old-space-size=8192' npx tsx modules/index.ts",
    "bench:router6": "pnpm prebench && BENCH_ROUTER=router6 NODE_OPTIONS='--expose-gc --max-old-space-size=8192' npx tsx modules/index.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint modules/ --fix --max-warnings 0"
  },
  "dependencies": {
    "router5": "8.0.1",
    "router6": "workspace:^"
  },
  "devDependencies": {
    "mitata": "1.0.34"
  }
}
