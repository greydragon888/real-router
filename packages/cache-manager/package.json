{
  "name": "@real-router/cache-manager",
  "version": "0.1.0",
  "type": "commonjs",
  "description": "Centralized cache registry with LRU eviction for real-router",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/esm/index.d.mts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": {
        "import": "./dist/esm/index.d.mts",
        "require": "./dist/cjs/index.d.ts"
      },
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "homepage": "https://github.com/greydragon888/real-router",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greydragon888/real-router.git"
  },
  "bugs": {
    "url": "https://github.com/greydragon888/real-router/issues"
  },
  "keywords": [
    "cache",
    "cache-manager",
    "real-router"
  ],
  "author": {
    "name": "Oleg Ivanov",
    "email": "greydragon888@gmail.com",
    "url": "https://github.com/greydragon888"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "lint": "eslint --cache --ext .ts src/ tests/ --fix --max-warnings 0 --no-error-on-unmatched-pattern",
    "test": "vitest run",
    "test:properties": "vitest run --config vitest.config.properties.mts"
  },
  "sideEffects": false
}
