{
  "name": "real-router",
  "version": "0.24.0",
  "type": "module",
  "description": "A real router. Just routing, nothing else.",
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "test:coverage": "turbo run test -- --coverage",
    "test:mutation": "stryker run",
    "lint": "turbo run lint",
    "lint:types": "turbo run lint:types",
    "lint:package": "turbo run lint:package",
    "lint:unused": "knip",
    "lint:duplicates": "jscpd packages/*/src/",
    "lint:deps": "syncpack lint",
    "lint:deps:fix": "syncpack fix-mismatches && pnpm dedupe && pnpm install",
    "lint:dedupe": "pnpm dedupe --check",
    "validate:pre-push": "turbo run build lint:package lint:types",
    "type-check": "turbo run type-check",
    "sonar": "dotenv -- sonar -Dsonar.login=$SONAR_TOKEN",
    "sonar:local": "pnpm type-check && pnpm test:coverage && pnpm sonar",
    "packages-structure": "find packages/*/src -type f -name \"*.ts*\" -print | tree --fromfile",
    "prepare": "husky || true",
    "size": "size-limit",
    "commit": "czg",
    "changeset": "changeset",
    "version": "changeset version && node .changeset/sync-version.mjs && node .changeset/aggregate-changelog.mjs",
    "release": "pnpm build && changeset publish",
    "danger:local": "danger local --dangerfile dangerfile.ts"
  },
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greydragon888/real-router.git"
  },
  "keywords": [
    "router",
    "routing",
    "reactive",
    "universal",
    "isomorphic",
    "framework-agnostic"
  ],
  "author": {
    "name": "Oleg Ivanov",
    "email": "greydragon888@gmail.com",
    "url": "https://github.com/greydragon888"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/greydragon888/real-router/issues"
  },
  "homepage": "https://github.com/greydragon888/real-router#readme",
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@changesets/changelog-github": "0.5.2",
    "@changesets/cli": "2.29.8",
    "@codspeed/vitest-plugin": "5.0.1",
    "@commitlint/cli": "20.3.1",
    "@commitlint/config-conventional": "20.3.1",
    "@eslint-react/eslint-plugin": "2.13.0",
    "@eslint/js": "9.39.2",
    "@fast-check/vitest": "0.2.4",
    "@size-limit/esbuild": "12.0.0",
    "@size-limit/preset-small-lib": "12.0.0",
    "@sonar/scan": "4.3.4",
    "@stryker-mutator/core": "9.4.0",
    "@stryker-mutator/typescript-checker": "9.4.0",
    "@stryker-mutator/vitest-runner": "9.4.0",
    "@stylistic/eslint-plugin": "5.7.1",
    "@types/node": "25.2.3",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/eslint-plugin": "1.6.9",
    "@vitest/ui": "4.0.18",
    "czg": "1.12.0",
    "danger": "13.0.5",
    "dotenv-cli": "11.0.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-config-turbo": "2.8.9",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-jsdoc": "62.4.1",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-sonarjs": "3.0.5",
    "eslint-plugin-testing-library": "7.15.4",
    "eslint-plugin-unicorn": "62.0.0",
    "fast-check": "4.5.3",
    "husky": "9.1.7",
    "jscpd": "4.0.7",
    "knip": "5.82.1",
    "mitata": "1.0.34",
    "prettier": "3.8.1",
    "publint": "0.3.17",
    "size-limit": "12.0.0",
    "syncpack": "13.0.4",
    "terser": "5.46.0",
    "tsup": "8.5.1",
    "turbo": "2.8.9",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.1",
    "vite-tsconfig-paths": "6.0.5",
    "vitest": "4.0.18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js",
      "esbuild",
      "fsevents",
      "unrs-resolver"
    ],
    "overrides": {
      "@types/node": "$@types/node",
      "@isaacs/brace-expansion": "5.0.1",
      "axios": ">=1.13.5",
      "qs": ">=6.14.2"
    }
  },
  "packageManager": "pnpm@10.24.0"
}
